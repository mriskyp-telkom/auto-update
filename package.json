{
  "name": "arkas-desktop",
  "productName": "arkas-desktop",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "BUILD=false electron-forge start",
    "package": "electron-forge package",
    "make": "BUILD=true electron-forge make",
    "make-win32": "BUILD=true electron-forge make --platform win32",
    "publish": "electron-forge publish",
    "lint:script": "eslint . --ext ts,tsx,js --fix --max-warnings=0",
    "postinstall": "husky install",
    "format": "prettier --write . "
  },
  "keywords": [],
  "author": {
    "name": "yasmin.ghassani",
    "email": "yasmin.ghassani@wartek.belajar.id"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "arkas_desktop"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "background": "./public/assets/logo-arkas.png",
            "format": "ULFO"
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-dmg": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "6.0.0-beta.61",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "autoprefixer": "^10.4.2",
    "clsx": "^1.1.1",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.0.0",
    "electron": "16.0.6",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "husky": "^7.0.4",
    "lint-staged": ">=10",
    "node-loader": "^2.0.0",
    "postcss-loader": "^6.2.1",
    "prettier": "2.5.1",
    "style-loader": "^3.0.0",
    "tailwindcss": "2.1.1",
    "ts-loader": "^9.2.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@wartek-id/button": "^1.1.1",
    "@wartek-id/design-tokens": "^1.4.1",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "zustand": "^3.5.10"
  }
}
